    <script>
      hljs.highlightAll();
    </script>
  </head>
  <body
    class="bg-gray-900 text-white min-h-screen flex items-center justify-center"
  >
    <!-- New wrapper div -->
    <div class="wrapper">
      <div class="main">
        <!-- The main element will act only as an isolated visual effect -->
      </div>
      <div class="content text-center flex space-x-10">
        <div class="flex flex-col justify-center items-center">
          <image src="/express.png" alt="express" class="w-40 my-4" />
          <image src="/sqlite-trek.png" alt="SQLite Trek" class="w-96" />
          <h1 class="text-4xl font-bold mb-8 text-yellow-400">
            Series Searcher
          </h1>

          <form
            action="/search"
            method="POST"
            autocomplete="off"
            class="bg-gray-800 p-6 rounded-lg shadow-lg"
          >
            <label
              for="search"
              class="block text-lg font-semibold mb-2 text-yellow-300"
            >
              Enter search term:
            </label>
            <input
              type="text"
              id="query"
              name="query"
              placeholder="e.g., Picard, Discovery"
              class="w-full p-3 rounded-md border border-gray-600 bg-gray-700 text-white focus:outline-none focus:ring-2 focus:ring-yellow-400 mb-4"
            />
            <button
              type="submit"
              class="w-full p-3 bg-yellow-400 text-gray-900 font-bold rounded-md hover:bg-yellow-400 focus:outline-none focus:ring-2 focus:ring-yellow-600 transition duration-200"
            >
              Engage Search
            </button>
          </form>
          <a href="<%= host %>" class="p-4 text-white">
            Try the tinyhttp version
          </a>
        </div>
        <% if (data) { %>
        <div class="my-8 flex flex-col justify-center items-center">
          <h2 class="text-2xl font-semibold mb-4">Query Results</h2>
